{
  "name": "state-grades",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix --max-warnings=0",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|md|html)\"",
    "scss-lint": "sass-lint src/**/*.scss -v -f table --max-warnings 0",
    "scss-lint:fix": "sass-lint-auto-fix src/**/*.scss -d -h -c .sass-lint-auto-fix.yml",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    " *.+(js|jsx)": [
      "npm run eslint:fix",
      "prettier --write \"**/*.+(js|jsx)\"",
      "git add ."
    ],
    "*.+(json|yml|yaml|md)": [
      "prettier --write \"**/*.+(json|yml|yaml|md)\"",
      "git add ."
    ],
    "src/**/*.+(scss)": [
      "npm run scss-lint:fix",
      "git add ."
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.0",
    "@testing-library/user-event": "^12.2.0",
    "bootstrap": "^4.5.3",
    "classnames": "^2.2.6",
    "d3-array": "^2.8.0",
    "d3-axis": "^2.0.0",
    "d3-ease": "^2.0.0",
    "d3-fetch": "^2.0.0",
    "d3-interpolate": "^2.0.1",
    "d3-scale": "^3.2.3",
    "d3-scale-chromatic": "^2.0.0",
    "d3-selection": "^2.0.0",
    "d3-shape": "^2.0.0",
    "d3-transition": "^2.0.0",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.0",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.0",
    "react-spring": "^8.0.27",
    "source-map-explorer": "^2.5.0"
  },
  "devDependencies": {
    "@welldone-software/why-did-you-render": "^6.0.0-rc.1",
    "eslint": "^7.13.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.2",
    "sass-lint": "^1.13.1",
    "sass-lint-auto-fix": "^0.21.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
